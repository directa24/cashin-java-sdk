<?xml version="1.0"?>
<FindBugsFilter>
    <Match>
        <Package name="~.*\.dao"/>
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON"/>
    </Match>
    <Match>
        <Package name="~.*\.filter.v1"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
        <Method name="doFilter"/>
    </Match>
    <Match>
        <Package name="~.*\.filter.v2"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
        <Method name="doFilter"/>
    </Match>
    <Match>
        <Package name="~.*\.filter"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
        <Method name="doFilter"/>
    </Match>
    <Match>
        <Package name="~.*\.mapper"/>
        <Bug pattern="NP_BOOLEAN_RETURN_NULL, NP_NULL_ON_SOME_PATH, NP_NULL_ON_SOME_PATH_MIGHT_BE_INFEASIBLE"/>
    </Match>
    <Match>
        <Class name="~.*\.MerchantPayerService"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
        <Method name="getOrCreateMerchantPayer"/>
    </Match>
</FindBugsFilter>
